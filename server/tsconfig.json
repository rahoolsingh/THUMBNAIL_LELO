{
    "_version": "22.0.0",

    "compilerOptions": {
        "lib": [
            "es2024",
            "ESNext.Array",
            "ESNext.Collection",
            "ESNext.Iterator"
        ],
        "module": "nodenext",
        "target": "es2022",

        "strict": false,
        "skipLibCheck": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "moduleResolution": "node16"
    }
}
